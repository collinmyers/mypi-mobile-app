{
    "expo": {
        "name": "myPI",
        "slug": "mypi",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/myPILogo.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./assets/myPILogo.png",
            "resizeMode": "contain",
            "backgroundColor": "#FFFFFF"
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.discoverpi.mypi",
            "infoPlist": {
                "UIUserInterfaceStyle": "Light"
            }
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/myPILogo.png",
                "backgroundColor": "#134C77"
            },
            "package": "com.discoverpi.mypi",
            "versionCode": 1,
            "googleServicesFile": "./google-services.json"
        },
        "web": {
            "favicon": "./assets/myPILogo.png"
        },
        "extra": {
        },
        "owner": "mypi"
    }
}